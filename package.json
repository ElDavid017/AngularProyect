{
  "name": "reportes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
  "start": "ng serve --proxy-config proxy.conf.json",
    "start:local": "ng serve -c local --port 4200",
    "start:remote": "ng serve -c remote --port 4202",
  "start:proxy": "ng serve --proxy-config proxy.conf.json --port 4200",
    "dev:server": "node --loader ts-node/esm src/server.ts",
    "dev:backend": "node dev-server.js",
    "dev:backend:local": "node dev-server.js .env",
    "dev:backend:remote": "node dev-server.js .env.remote",
  "dev:all": "ng serve --proxy-config proxy.conf.json --port 4200",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:reportes": "node dist/reportes/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.3",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.3",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.3",
    "@angular/ssr": "^20.3.4",
    "bootstrap": "^5.3.8",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mysql2": "^3.15.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.4",
    "@angular/cli": "^20.3.4",
    "@angular/compiler-cli": "^20.3.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.19.20",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "sass": "^1.93.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  }
}
